    "chalk": "4.1.2",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "eslint": "^9.17.0",
    "openai": "^4.82.0",
    "prettier": "^3.5.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.29.1",
    "tgrid": "^1.1.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tstl": "^3.0.0",
